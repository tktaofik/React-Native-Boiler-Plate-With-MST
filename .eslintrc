{
  "root": true,
  "env": {
    "es6": true
  },
  "parser": "babel-eslint",
  "extends": "airbnb",
  "globals": {
    "__DEV__": true,
    "require": true,
    "window": true,
    "expect": true,
    "test": true,
    "it": true,
    "afterAll": true,
    "describe": true
  },
  "rules": {
    "brace-style":0,
    "semi":0,
    "no-console": 0,
    "no-useless-escape": 0,
    "react/jsx-boolean-value": 0,
    "react/no-string-refs": 0,
    "class-methods-use-this": 0,
    "no-trailing-spaces": 0,
    "no-plusplus": 0,
    "no-underscore-dangle": 0,
    "react/prop-types": 0,
    "react/jsx-tag-spacing": 0,
    "max-len": 0,
    "no-else-return": 0,
    "no-param-reassign": 0,
    "no-shadow": 0,
    "no-mixed-operators": 0,
    "padded-blocks": 0,
    "arrow-body-style": 0,
    "import/no-name-as-default": 0,
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [
          ".js",
          ".jsx"
        ]
      }
    ]
  }
}
